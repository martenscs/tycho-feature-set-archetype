<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	name="__rootArtifactId__-parent"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<requiredProperties>
		<requiredProperty key="providerName" />
		<requiredProperty key="categoryId" />
		<requiredProperty key="webContext"/>
	</requiredProperties>
	<fileSets>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-prepare</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-prepare/lib</directory>

		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-build</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-website/src</directory>
			<includes>
				<include>**/*.jsp</include>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__</directory>
			<includes>
				<include>**/*.xml</include>
				<include>**/*.properties</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-website</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-repository</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-target</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>__package__-target</directory>
			<includes>
				<include>**/*.target</include>
			</includes>
		</fileSet>
	</fileSets>
</archetype-descriptor>
